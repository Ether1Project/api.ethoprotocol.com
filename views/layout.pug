    meta(charset="utf-8")
    title ETHO API REST server
    if title=="Etho Protocol API"
      meta(http-equiv="refresh", content="5;url=https://docs.ethoprotocol.com/information/api-documentation")
    meta(name="description", content="Etho Protocol API REST server")
    meta(name="keywords", content="Crypto currency, api, etho protocol")
    meta(property="og:title", content="Etho Protocol API REST server")
    meta(property="og:description", content="Etho Protocol API REST server description and definition")
    meta(property="og:image", content="imgages/etho.png")
    meta(property="og:url", content="https://api.ethoprotocol.com")
    meta(name="twitter:card", content="imgages/etho.png")
